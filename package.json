{
  "name": "@flubs/ionic4-phone-spinner",
  "version": "1.0.0",
  "scripts": {
    "build": "ng-packagr -p package.json && cp src/scss/ionic4-phone-spinner.scss dist/ionic4-phone-spinner.scss && cp -r src/assets dist/assets",
    "build:demo": "npm run build && cd demo && cp -fr ../dist/* node_modules/ionic4-phone-spinner/ && cd ..",
    "build-run:demo": "npm run build:demo && npm run run:demo",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ionic4-phone-spinner -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "publish": "npm publish dist",
    "run:demo": "cd demo && ionic serve",
    "shipit": "npm run lint && npm run test && npm run build:demo && npm run docs:build && npm run publish",
    "test": "tsc && karma start"
  },
  "ngPackage": {
    "lib": {
      "cssUrl": "inline",
      "entryFile": "src/index.ts",
      "styleIncludePaths": [
        "./src/scss"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrquick17/ionic4-phone-spinner"
  },
  "author": {
    "name": "Jeremy Quick",
    "email": "me@jrquick.com"
  },
  "keywords": [
    "angular",
    "autocomplete",
    "ionic"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrquick17/ionic4-phone-spinner/issues"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.803.19",
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/core": "~8.3.19",
    "@angular-devkit/schematics": "~8.3.19",
    "@angular/animations": "^8.2.11",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.19",
    "@angular/common": "^8.2.6",
    "@angular/compiler": "^8.2.11",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.6",
    "@angular/forms": "^8.2.6",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.6",
    "@angular/platform-browser-dynamic": "^8.2.6",
    "@angular/router": "^8.2.6",
    "@angular/service-worker": "^8.2.6",
    "@compodoc/compodoc": "^1.1.11",
    "@ionic-native/core": "5.17.0",
    "@ionic/angular": "^4.11.5",
    "@ionic/angular-toolkit": "^2.1.1",
    "@ionic/pro": "2.0.4",
    "@ionic/storage": "2.2.0",
    "@types/core-js": "^2.5.2",
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.12.12",
    "codelyzer": "~5.2.0",
    "cordova-plugin-ionic": "^5.4.5",
    "fsevents": "^2.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
